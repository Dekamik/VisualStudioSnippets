<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
      <Title>xUnit Fact Test Method</Title>
      <Shortcut>fact</Shortcut>
      <Description>Code snippet for a xUnit fact test method</Description>
      <Author>Microsoft Corporation</Author>
    </Header>
    <Snippet>
      <Imports>
        <Import>
		  <Namespace>FluentAssertions</Namespace>
          <Namespace>Xunit</Namespace>
        </Import>
      </Imports>
      <Declarations>
        <Literal>
          <ID>method</ID>
          <ToolTip>Name of the method to test</ToolTip>
          <Default>Method</Default>
        </Literal>
		<Literal>
		  <ID>condition</ID>
		  <ToolTip>The conditions we're testing in this test</ToolTip>
		  <Default>Condition</Default>
		</Literal>
		<Literal>
		  <ID>results</ID>
		  <ToolTip>The expected results from this test</ToolTip>
		  <Default>Results</Default>
		</Literal>
	    <Literal Editable="false">
		  <ID>Fact</ID>
		  <Function>SimpleTypeName(global::Xunit.Fact)</Function>
	    </Literal>
      </Declarations>
      <Code Language="csharp"><![CDATA[[$Fact$]
      public void $method$_$condition$_$results$()
      {
		  		  
      }]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>